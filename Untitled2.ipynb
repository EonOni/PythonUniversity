    "from multipledispatch import dispatch\n",
    "\n",
    "@dispatch(object, object)\n",
    "def add(x, y):\n",
    "    return f\"{x} + {y}\"\n",
    "@dispatch(int, int)\n",
    "def add(x, y):\n",
    "    return x + y\n",
    "\n",
    "\n",
    "add(1, 'hello')"
   ]
